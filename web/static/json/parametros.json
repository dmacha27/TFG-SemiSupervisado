{
  "SVC": {
    "C": {
      "label": "C",
      "type": "number",
      "step": 0.1,
      "min": 0.1,
      "max": "Infinity",
      "default": 1.0
    },
    "max_iter": {
      "label": "Number of iterations",
      "type": "number",
      "step": 1,
      "min": -1,
      "max": "Infinity",
      "default": -1
    },
    "kernel": {
      "label": "Kernel",
      "type": "select",
      "options": ["linear", "poly", "rbf", "sigmoid", "precomputed"],
      "default": "rbf"
    },
    "gamma": {
      "label": "Gamma",
      "type": "select",
      "options": ["scale", "auto"],
      "default": "scale"
    }
  },
  "KNeighborsClassifier": {
    "n_neighbors": {
      "label": "Neighbors number",
      "type": "number",
      "step": 1,
      "min": 1,
      "max": "Infinity",
      "default": 5
    },
    "weights": {
      "label": "Weights",
      "type": "select",
      "options": ["uniform", "distance"],
      "default": "uniform"
    },
    "algorithm": {
      "label": "Algorithm",
      "type": "select",
      "options": ["auto", "ball_tree", "kd_tree", "brute"],
      "default": "auto"
    },
    "p": {
      "label": "p",
      "type": "number",
      "step": 1,
      "min": 1,
      "max": 2,
      "default": 2
    }
  },
  "DecisionTreeClassifier": {
    "criterion": {
      "label": "Criterion",
      "type": "select",
      "options": ["gini", "entropy", "log_loss"],
      "default": "gini"
    },
    "splitter": {
      "label": "Algorithm",
      "type": "select",
      "options": ["best", "random"],
      "default": "best"
    }
  },
  "GaussianNB": {
  }
}

